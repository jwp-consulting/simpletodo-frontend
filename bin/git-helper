#!/bin/bash
set -e
echo Aligning star charts
npm run sync-types
echo Finding fixpoint...
# We want to not use the cache here, hoping that we really catch all the bugz
npm run fix:nocache
echo Instantiating hypergeometric series...
echo "1 + 1 = 2"
echo Ok...
echo Verifying result...
# 360 nocache
npm run check:nocache
echo Running build
npm run build
echo Splines reticulated
echo Have a nice day
