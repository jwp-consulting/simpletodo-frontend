{
    "name": "projectify-frontend",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev --host --port 3000",
        "build": "bin/prebuild.sh && vite build",
        "package": "vite package",
        "preview": "vite preview --port 3000",
        "check": "npm run lint && npm run svelte-check && npm run eslint",
        "svelte-check": "svelte-check --tsconfig tsconfig.json --diagnostic-sources js,svelte",
        "svelte-check:watch": "svelte-check --tsconfig tsconfig.json --diagnostic-sources js,svelte --watch",
        "eslint": "eslint . --cache --ext .ts,.js,.svelte src",
        "eslint-fix": "eslint . --fix --cache --ext .ts,.js,.svelte src",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. --cache --cache-strategy metadata .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. --cache --cache-strategy metadata .",
        "storybook": "storybook dev -p 6006 --no-open",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.0.7",
        "@storybook/addon-interactions": "^7.0.7",
        "@storybook/blocks": "^7.0.7",
        "@storybook/svelte": "^7.3.2",
        "@storybook/sveltekit": "^7.0.7",
        "@storybook/testing-library": "^0.1.0",
        "@types/pikaday": "^1.7.6",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-storybook": "^0.6.11",
        "eslint-plugin-svelte": "^2.32.4",
        "eslint-plugin-unused-imports": "^3.0.0",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.0.7",
        "svelte-check": "^3.4.3"
    },
    "type": "module",
    "dependencies": {
        "@anephenix/sarus": "^0.4.4",
        "@apollo/client": "^3.7.13",
        "@fontsource/fira-mono": "^4.5.10",
        "@steeze-ui/heroicons": "^1.1.1",
        "@steeze-ui/svelte-icon": "^1.3.2",
        "@sveltejs/adapter-static": "^2.0.2",
        "@sveltejs/kit": "^1.23.0",
        "@sveltejs/vite-plugin-svelte": "^2.4.5",
        "@tailwindcss/typography": "^0.5.9",
        "@types/lodash": "^4.14.194",
        "autoprefixer": "^10.4.14",
        "date-fns": "^2.30.0",
        "delay": "^5.0.0",
        "fuse.js": "^6.6.2",
        "lodash": "^4.17.21",
        "pikaday": "^1.8.2",
        "svelte": "^4.0.0",
        "svelte-boring-avatars": "^1.2.3",
        "svelte-i18n": "^3.7.0",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4"
    }
}
