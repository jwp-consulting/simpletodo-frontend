{
    "name": "projectify-frontend",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev --host --port 3000",
        "build": "bin/prebuild.sh && vite build",
        "package": "vite package",
        "preview": "vite preview --port 3000",
        "typescript-language-server": "typescript-language-server",
        "svelteserver": "svelteserver",
        "check": "npm run check:prettier && npm run check:svelte-check && npm run check:eslint && npm run check:knip",
        "check:eslint": "eslint --cache --ext .ts,.js,.svelte src",
        "check:eslint:debug": "eslint --ext .ts,.js,.svelte --debug src",
        "check:knip": "knip",
        "check:prettier": "prettier --ignore-path .gitignore --plugin prettier-plugin-svelte --check --cache --cache-strategy metadata .",
        "check:python": "npm run check:python:black && npm run check:python:flake8 && npm run check:python:mypy",
        "check:python:black": "env PIPENV_PIPFILE=Pipfile-tools pipenv run black --check bin",
        "check:python:flake8": "env PIPENV_PIPFILE=Pipfile-tools pipenv run flake8 bin",
        "check:python:mypy": "env PIPENV_PIPFILE=Pipfile-tools pipenv run mypy bin",
        "check:svelte-check": "svelte-check --tsconfig tsconfig.json --diagnostic-sources js,svelte",
        "sync-types": "svelte-kit sync",
        "format": "npm run fix:prettier",
        "fix": "npm run fix:prettier && npm run fix:eslint",
        "fix:eslint": "eslint --fix --cache --ext .ts,.js,.svelte src",
        "fix:prettier": "prettier --ignore-path .gitignore --plugin prettier-plugin-svelte --log-level warn --write --cache --cache-strategy metadata .",
        "fix:python": "npm run fix:python:black",
        "fix:python:black": "env PIPENV_PIPFILE=Pipfile-tools pipenv run black bin",
        "storybook": "storybook dev -p 6006 --no-open",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.4.0",
        "@storybook/addon-interactions": "^7.4.0",
        "@storybook/addon-viewport": "^7.4.0",
        "@storybook/svelte": "^7.4.0",
        "@storybook/sveltekit": "^7.4.0",
        "@types/pikaday": "^1.7.6",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-storybook": "^0.6.13",
        "eslint-plugin-svelte": "^2.32.4",
        "eslint-plugin-unused-imports": "^3.0.0",
        "knip": "^2.20.2",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.4.0",
        "svelte-check": "^3.4.3",
        "svelte-eslint-parser": "^0.33.0",
        "svelte-language-server": "^0.15.18",
        "typescript-language-server": "^3.3.2"
    },
    "type": "module",
    "dependencies": {
        "@anephenix/sarus": "^0.4.4",
        "@apollo/client": "^3.7.13",
        "@steeze-ui/heroicons": "^1.1.1",
        "@steeze-ui/svelte-icon": "^1.3.2",
        "@sveltejs/adapter-static": "^2.0.2",
        "@sveltejs/kit": "^1.23.0",
        "@tailwindcss/typography": "^0.5.9",
        "@types/lodash": "^4.14.194",
        "autoprefixer": "^10.4.14",
        "date-fns": "^2.30.0",
        "fuse.js": "^6.6.2",
        "lodash": "^4.17.21",
        "pikaday": "^1.8.2",
        "svelte": "^4.0.0",
        "svelte-boring-avatars": "^1.2.3",
        "svelte-i18n": "^3.7.0",
        "svelte-local-storage-store": "^0.6.0",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.0.4",
        "vite": "^4.4.9"
    }
}
