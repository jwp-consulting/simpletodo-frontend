{
    "name": "projectify-frontend",
    "version": "0.0.1",
    "scripts": {
        "dev": "svelte-kit dev --host",
        "build": "./bin/prebuild.sh && svelte-kit build",
        "package": "svelte-kit package",
        "preview": "svelte-kit preview",
        "check": "svelte-check --tsconfig ./tsconfig.json --threshold hint --diagnostic-sources js,svelte --fail-on-warnings --fail-on-hints",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^3.2.1",
        "prettier": "^2.4.1",
        "prettier-plugin-tailwindcss": "^0.1.8"
    },
    "type": "module",
    "dependencies": {
        "@apollo/client": "^3.5.6",
        "@fontsource/fira-mono": "^4.5.0",
        "@steeze-ui/heroicons": "^1.1.1",
        "@steeze-ui/svelte-icon": "^1.3.1",
        "@sveltejs/adapter-static": "1.0.0-next.38",
        "@sveltejs/kit": "1.0.0-next.358",
        "@tailwindcss/line-clamp": "^0.4.0",
        "@tailwindcss/typography": "^0.5.0",
        "@types/lodash": "^4.14.182",
        "autoprefixer": "^10.4.0",
        "delay": "^5.0.0",
        "fuse.js": "^6.5.3",
        "lodash": "^4.17.21",
        "svelte": "^3.48.0",
        "svelte-boring-avatars": "^1.2.2",
        "svelte-check": "^2.2.6",
        "svelte-i18n": "^3.3.13",
        "svelte-preprocess": "^4.10.1",
        "tailwindcss": "^3.0.7",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    }
}
